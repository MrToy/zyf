.box
	background: url(./p6.png) no-repeat bottom #f8f2dc;padding: 20px 0 200px
	.center
		width: 1070px;margin: 0 auto;overflow: hidden
	.full
		width:100%
	.radius
		width: 230px;height: 230px;border-radius: 115px;overflow: hidden;margin: 0 10px;border: 4px solid #fff
	p
		font-size: 20px;margin: 20px 0 0;text-align: center;color: #000
	.map
		position: relative
		a
			width: 450px;height: 270px;position: absolute
			&:nth-child(1)
				top: 140px;left: 70px
			&:nth-child(2)
				top: 140px;left: 570px
			&:nth-child(3)
				top: 500px;left: 70px
			&:nth-child(4)
				top: 500px;left: 570px
			&:nth-child(5)
				top: 860px;left: 70px
	.imgs
		text-align:center
.date
	color:#888;float:right
.pagination
	display: inline-block;padding-left: 0;margin: 20px 0;border-radius: 4px
	li
		display: inline
		a
			cursor: pointer
		&.disabled a
			cursor: default;color:#bbb
		a,span
			outline: none;position: relative;float: left;padding: 6px;margin:0 5px;line-height: 1.42857143;text-decoration: none
	.active
		a,a:focus,a:hover,span,span:focus,span:hover
			z-index: 3;cursor: default;border-bottom: 2px solid

.box
	color: #080806;border-bottom: 1px solid #555;padding-bottom: 10px
	&:after
		content: ".";display: block;height: 0;visibility: hidden;clear: both
	img
		float: left;
	.text
		float: left;padding-left: 15px;
		.title
			font-size: 22px;font-weight: 700
		.info
			font-size: 10px;color: #555;line-height: 10px
	a
		float: right;margin-top: 20px;font-size: 15px;color: #666;